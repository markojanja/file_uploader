<!DOCTYPE html>
<html lang="en">
  <%- include("partials/head") %>
  <body>
    <%- include("partials/header") %>
    <main>
      <h1>Dash</h1>

      <a href="dashboard/create">Add new folder</a>
      <ul>
        <% data.forEach(item => { %>
        <li>
          <a href="<%= item.type === 'FOLDER' ? '/dashboard/folders' : '/dashboard/files' %>">
            <%= item.name %>
          </a>
        </li>
        <% }) %>
      </ul>
    </main>
  </body>
</html>
