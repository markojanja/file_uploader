<!DOCTYPE html>
<html lang="en">
  <%- include("partials/head") %>
  <body>
    <%- include("partials/header") %>
    <main>
      <h1>Dash</h1>

      <a href="/dashboard<%= parentId ? '/' + parentId + '/create' : '/create' %>"
        >Add new folder</a
      >
      <a href="/dashboard<%= parentId ? '/' + parentId + '/upload' : '/upload' %>">Upload file</a>
      <ul>
        <% data.forEach(item => { %>
        <li>
          <a href="<%= item.type === 'FOLDER' ? '/dashboard/' + item.id : item.url %>">
            <%= item.name %>
          </a>
        </li>
        <% }) %>
      </ul>
    </main>
  </body>
</html>
